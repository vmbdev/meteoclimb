{
  "name": "meteoclimb",
  "type": "module",
  "version": "1.0.7",
  "scripts": {
    "ui-start": "vite",
    "ui-build": "vite build",
    "ui-serve": "vite preview",
    "server-dev": "nodemon --ext js --exec node ./backend/server.js --watch ./backend",
    "server-prod": "NODE_ENV=production node ./backend/server.js",
    "docs-backend": "jsdoc -c ./jsdoc-backend.json",
    "docs-frontend": "jsdoc -c ./jsdoc-frontend.json",
    "formatjs-extract": "formatjs extract ./frontend/src/**/*.js* --ignore=**/*.d.js --ignore=./frontend/src/locales/** --ignore=./frontend/src/translations/** --out-file ./frontend/src/translations/en.json --id-interpolation-pattern [sha512:contenthash:base64:6]",
    "formatjs-compile": "formatjs compile",
    "formatjs-compile-all": "formatjs compile-folder ./frontend/src/translations ./frontend/src/locales",
    "db-migrate": "sequelize-cli db:migrate",
    "db-seed": "sequelize-cli db:seed:all"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "luxon": "^3.2.1",
    "nodemon": "^3.0.0",
    "pg": "^8.8.0",
    "sequelize": "^6.31.0"
  },
  "devDependencies": {
    "@formatjs/cli": "^6.1.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@vitejs/plugin-react": "^4.1.0",
    "ky": "^1.1.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-intl": "^6.2.1",
    "react-toastify": "^9.1.3",
    "sass": "^1.56.1",
    "sequelize-cli": "^6.6.1",
    "vite": "^5.0.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  }
}
